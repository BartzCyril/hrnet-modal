  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/BartzCyril/hrnet-modal"
  },
  "keywords": [
    "react",
    "typescript",
    "modal"
  ],
  "author": "Cyril BARTZ",
  "license": "ISC",
  "homepage": "https://github.com/BartzCyril/hrnet-modal/blob/main/README.md",
